#!/usr/bin/python3
from sys import argv

if len(argv) != 2:
    print("Usage: ./factor <filename>")
    exit(1)

filename = argv[1]

with open(filename) as f:
    for line in  f:
        num = int(line)
        print("{:d}=".format(num), end='')
        if num % 2 == 0:
            print("{}*2".format(num // 2))
            continue
        for i in range(3, num, 2):
            if num % i == 0:
                factor = num//i
                for a in range(3, factor, 2):
                    if factor % a == 0 or a == 0:
                        break
                else:
                    print("{}*{}".format(factor, i))
                    break
