#!/usr/bin/python3

from sys import argv

if len(argv) != 2:
    print("Usage: ./factors <filename>")
    exit(1)

filename = argv[1]

with open(filename) as f:
    for line in f:
        num = int(line.strip())
        print("{:d}=".format(num), end='')

        if num % 2 == 0:
            print("{}*2".format(num//2))
            continue

        for i in range(3, num, 2):
            if num % i == 0:
                factors = num//i
                for j in range(3, factors, 2):
                    if factors % j == 0:
                        break
                else:
                    print("{}*{}".format(factors, i))
                break


